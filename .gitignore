# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-*

# Ignore all log files and temp files.
/log/*.log
/tmp

# Ignore public/uploads directory if you use file uploads.
/public/uploads

# Ignore the local application configuration file.
/config/application.yml

# Ignore the master key for decrypting credentials and secrets.
/config/master.key

# Ignore environment-specific files.
.env*
.env.local
.env.development
.env.test
.env.production

# Ignore any local database config.
/config/database.yml

# Ignore puma config file.
/config/puma.rb

# Ignore yarn integrity file.
/yarn-error.log
/yarn.lock

# Ignore node_modules.
/node_modules

# Ignore .sass-cache.
/.sass-cache

# Ignore .DS_Store files.
.DS_Store

# Ignore coverage reports generated by SimpleCov.
/coverage

# Ignore public/packs directory if you're using Webpacker for asset management.
/public/packs

# Ignore Mac-specific files.
.AppleDouble
.DS_Store
.LSOverride

# Ignore .vscode folder if you use Visual Studio Code.
.vscode

# Ignore .idea folder if you use JetBrains IDEs.
.idea

# Ignore environment configuration files.
/config/environments/*.local
/config/environments/*.local.*
